{
	"name": "@vyke/fns",
	"type": "module",
	"version": "0.0.1",
	"author": "Jose Albizures <jose@albizures.com> (https://albizures.com)",
	"license": "MIT",
	"keywords": [],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./calculations": {
			"import": "./dist/calculations.mjs",
			"require": "./dist/calculations.cjs"
		},
		"./array/filter": {
			"import": "./dist/array/filter.mjs",
			"require": "./dist/array/filter.cjs"
		},
		"./array/sort": {
			"import": "./dist/array/sort.mjs",
			"require": "./dist/array/sort.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "run-s build:*",
		"build:dist": "unbuild",
		"build:docs": "tsdocs",
		"test": "run-s test:unit test:types test:lint",
		"test:use": "knip",
		"test:lint": "eslint .",
		"test:unit": "vitest run",
		"test:unit:dev": "vitest",
		"test:types": "tsc --noEmit",
		"prepack": "pnpm build",
		"release:bump": "standard-version -t v",
		"release:publish": "pnpm publish --access public",
		"release:jsr-sync": "jsr-sync --section=exports",
		"release:push-tags": "git push --follow-tags origin main",
		"release": "run-s test build release:jsr-sync release:bump release:publish release:push-tags"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.19.0",
		"@types/node": "^20.12.12",
		"@vyke/jsr-sync": "^0.0.3",
		"@vyke/tsdocs": "^0.0.8",
		"eslint": "^9.3.0",
		"knip": "^5.17.2",
		"npm-run-all": "^4.1.5",
		"standard-version": "^9.5.0",
		"typescript": "^5.4.5",
		"unbuild": "^2.0.0",
		"vitest": "^1.6.0"
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	}
}
